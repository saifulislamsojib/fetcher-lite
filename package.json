{
  "name": "fetcher-lite",
  "version": "0.0.1",
  "description": "Fetcher Lite - Universal Fetch Wrapper",
  "main": "dist/index.js",
  "scripts": {
    "install:pkg": "pnpm install --frozen-lockfile",
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit && tsc -p src/tests/tsconfig.json --noEmit",
    "lint": "eslint --max-warnings 0 --fix",
    "lintcheck": "eslint --max-warnings 0",
    "format": "prettier --log-level warn --write \"./**/*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}\"",
    "test": "vitest run",
    "test:w": "vitest --watch",
    "coverage": "vitest run --coverage",
    "lint-staged": "lint-staged",
    "prepare": "husky || true"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./Fetcher": {
      "types": "./dist/Fetcher.d.ts",
      "default": "./dist/Fetcher.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "keywords": [],
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.10",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@types/node": ">=18",
    "typescript": ">=4"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,d.ts,mts}": "eslint --max-warnings 0 --fix",
    "*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}": "prettier --log-level warn --write"
  }
}
