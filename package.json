{
  "name": "fetcher",
  "version": "0.0.1",
  "description": "fetcher",
  "main": "dist/index.js",
  "scripts": {
    "install:pkg": "pnpm install --frozen-lockfile",
    "preinstall": "npx only-allow pnpm",
    "start": "node dist/index.js",
    "dev": "tsx watch --clear-screen=false --include './src/**/*' -r tsconfig-paths/register src/index.ts",
    "build": "tsc && tsc-alias",
    "typecheck": "tsc --noEmit && tsc -p src/tests/tsconfig.json --noEmit",
    "lint": "eslint --max-warnings 0 --fix",
    "lintcheck": "eslint",
    "format": "prettier --log-level warn --write \"./**/*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}\"",
    "test": "vitest run",
    "test:w": "vitest --watch",
    "coverage": "vitest run --coverage",
    "lint-staged": "lint-staged",
    "prepare": "husky || true"
  },
  "keywords": [],
  "author": "Saiful Islam Sojib",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.30.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,d.ts,mts}": "eslint --max-warnings 0 --fix",
    "*.{js,mjs,cjs,ts,d.ts,mts,json,yaml,yml,html}": "prettier --log-level warn --write"
  }
}
